<insightmakermodel>\n  <root> \n<header> model_id="2QGhZudDQQF5e3wUBEAD75", model_title="A Business Model", model_author_id="1GJLNCIsD2GRCtSTVhTzDS", model_author_name="Alyssa Zhao" </header>\n    <mxCell id="0"/>\n    <mxCell id="1" parent="0"/>\n    <Folder name="Scenarios" Note="" Type="None" Solver="{&quot;enabled&quot;: false, &quot;algorithm&quot;: &quot;RK1&quot;, &quot;timeStep&quot;: 1}" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" AgentBase="" id="80">\n      <mxCell style="folder;fontFamily=Garamond;fontSize=20;fontStyle=2" parent="1" vertex="1">\n        <mxGeometry x="82" y="520" width="790" height="90" as="geometry"/>\n      </mxCell>\n    </Folder>\n    <Button name="Figure 30" Note="" Function="//delays&#xa;setValue(findName(&quot;delivery delay&quot;), 0);&#xa;setValue(findName(&quot;response delay&quot;), 0);&#xa;setValue(findName(&quot;perception delay&quot;), 0);&#xa;&#xa;setValue(findName(&quot;inventory of cars on the lot&quot;), 200);&#xa;setValue(findName(&quot;customer demand&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, 1.1, 1)&quot;);&#xa;//should be 10 * perceived sales to cover 10 days of sales&#xa;setValue(findName(&quot;desired inventory&quot;), &quot;10 * [perceived sales]&quot;);&#xa;setValue(findName(&quot;discrepancy&quot;), &quot;[desired inventory]-[inventory of cars on the lot]&quot;);&#xa;setValue(findName(&quot;sales&quot;), &quot;20 * [customer demand]&quot;);&#xa;//use Delay later&#xa;setValue(findName(&quot;perceived sales&quot;), &quot;[sales]&quot;);&#xa;setValue(findName(&quot;orders to factory&quot;), &quot;[discrepancy] + [perceived sales]&quot;);&#xa;setValue(findName(&quot;deliveries&quot;),&quot;[orders to factory]&quot;);&#xa;&#xa;closeAllWindows();&#xa;runModel();" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" oldId="78" id="78">\n      <mxCell style="button;fontFamily=Garamond;fontSize=18;" parent="80" vertex="1">\n        <mxGeometry x="17" y="30" width="120" height="40" as="geometry"/>\n      </mxCell>\n    </Button>\n    <Button name="Figure 32" Note="" Function="//delays&#xa;setValue(findName(&quot;delivery delay&quot;), 5);&#xa;setValue(findName(&quot;response delay&quot;), 3);&#xa;setValue(findName(&quot;perception delay&quot;), 5);&#xa;&#xa;setValue(findName(&quot;inventory of cars on the lot&quot;), 200);&#xa;setValue(findName(&quot;customer demand&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, 1.1, 1)&quot;);&#xa;setValue(findName(&quot;desired inventory&quot;), &quot;10 * [perceived sales]&quot;);&#xa;setValue(findName(&quot;discrepancy&quot;), &quot;[desired inventory]-[inventory of cars on the lot]&quot;);&#xa;&#xa;setTimeStep(0.15);&#xa;setValue(findName(&quot;sales&quot;), &quot;20 * [customer demand]&quot;);&#xa;setValue(findName(&quot;perceived sales&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, Delay1([sales], [perception delay]), [sales])&quot;);&#xa;setValue(findName(&quot;orders to factory&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, Delay1([discrepancy], [response delay]) + Delay1([perceived sales], [response delay]), [discrepancy] + [perceived sales])&quot;);&#xa;&#xa;setValue(findName(&quot;deliveries&quot;),&quot;IfThenElse(Time() &gt; {25 Days}, Delay1([orders to factory], [delivery delay]), [orders to factory])&quot;);&#xa;&#xa;closeAllWindows();&#xa;runModel();" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" oldId="81" id="81">\n      <mxCell style="button;fontFamily=Garamond;fontSize=18;" parent="80" vertex="1">\n        <mxGeometry x="174" y="30" width="120" height="40" as="geometry"/>\n      </mxCell>\n    </Button>\n    <Button name="Figure 34" Note="" Function="//delays&#xa;setValue(findName(&quot;delivery delay&quot;), 5);&#xa;setValue(findName(&quot;response delay&quot;), 3);&#xa;setValue(findName(&quot;perception delay&quot;), 2);&#xa;&#xa;setValue(findName(&quot;inventory of cars on the lot&quot;), 200);&#xa;setValue(findName(&quot;customer demand&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, 1.1, 1)&quot;);&#xa;//should be 10 * perceived sales to cover 10 days of sales&#xa;setValue(findName(&quot;desired inventory&quot;), &quot;10 * [perceived sales]&quot;);&#xa;setValue(findName(&quot;discrepancy&quot;), &quot;[desired inventory]-[inventory of cars on the lot]&quot;);&#xa;setValue(findName(&quot;sales&quot;), &quot;20 * [customer demand]&quot;);&#xa;setValue(findName(&quot;perceived sales&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, Delay1([sales], [perception delay]), [sales])&quot;);&#xa;setValue(findName(&quot;orders to factory&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, Delay1([discrepancy], [response delay]) + Delay1([perceived sales], [response delay]), [discrepancy] + [perceived sales])&quot;);&#xa;setValue(findName(&quot;deliveries&quot;),&quot;IfThenElse(Time() &gt; {25 Days}, Delay1([orders to factory], [delivery delay]), [orders to factory])&quot;);&#xa;setTimeStep(0.15);&#xa;closeAllWindows();&#xa;runModel();" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" oldId="82" id="82">\n      <mxCell style="button;fontFamily=Garamond;fontSize=18;" parent="80" vertex="1">\n        <mxGeometry x="334" y="30" width="120" height="40" as="geometry"/>\n      </mxCell>\n    </Button>\n    <Button name="Figure 35" Note="" Function="//delays&#xa;setValue(findName(&quot;delivery delay&quot;), 5);&#xa;setValue(findName(&quot;response delay&quot;), 2);&#xa;setValue(findName(&quot;perception delay&quot;), 5);&#xa;&#xa;setValue(findName(&quot;inventory of cars on the lot&quot;), 200);&#xa;setValue(findName(&quot;customer demand&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, 1.1, 1)&quot;);&#xa;//should be 10 * perceived sales to cover 10 days of sales&#xa;setValue(findName(&quot;desired inventory&quot;), &quot;10 * [perceived sales]&quot;);&#xa;setValue(findName(&quot;discrepancy&quot;), &quot;[desired inventory]-[inventory of cars on the lot]&quot;);&#xa;setTimeStep(0.25);&#xa;setValue(findName(&quot;sales&quot;), &quot;20 * [customer demand]&quot;);&#xa;setValue(findName(&quot;perceived sales&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, Delay3([sales], [perception delay]), [sales])&quot;);&#xa;setValue(findName(&quot;orders to factory&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, Delay3([discrepancy], [response delay]) + Delay3([perceived sales], [response delay]), [discrepancy] + [perceived sales])&quot;);&#xa;setValue(findName(&quot;deliveries&quot;),&quot;IfThenElse(Time() &gt; {25 Days}, Delay1([orders to factory], [delivery delay]), [orders to factory])&quot;);&#xa;&#xa;closeAllWindows();&#xa;runModel();" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" oldId="83" id="83">\n      <mxCell style="button;fontFamily=Garamond;fontSize=18;" parent="80" vertex="1">\n        <mxGeometry x="494" y="30" width="120" height="40" as="geometry"/>\n      </mxCell>\n    </Button>\n    <Button name="Figure 36" Note="" Function="//delays&#xa;setValue(findName(&quot;delivery delay&quot;), 5);&#xa;setValue(findName(&quot;response delay&quot;), 6);&#xa;setValue(findName(&quot;perception delay&quot;), 5);&#xa;&#xa;setValue(findName(&quot;inventory of cars on the lot&quot;), 200);&#xa;setValue(findName(&quot;customer demand&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, 1.1, 1)&quot;);&#xa;//should be 10 * perceived sales to cover 10 days of sales&#xa;setValue(findName(&quot;desired inventory&quot;), &quot;10 * [perceived sales]&quot;);&#xa;setValue(findName(&quot;discrepancy&quot;), &quot;[desired inventory]-[inventory of cars on the lot]&quot;);&#xa;setValue(findName(&quot;sales&quot;), &quot;20 * [customer demand]&quot;);&#xa;setValue(findName(&quot;perceived sales&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, Delay1([sales], [perception delay]), [sales])&quot;);&#xa;setValue(findName(&quot;orders to factory&quot;), &quot;IfThenElse(Time() &gt; {25 Days}, [discrepancy] + Delay1([perceived sales], [response delay]), [discrepancy] + [perceived sales])&quot;);&#xa;setValue(findName(&quot;deliveries&quot;),&quot;IfThenElse(Time() &gt; {25 Days}, Delay1([orders to factory], [delivery delay]), [orders to factory])&quot;);&#xa;&#xa;setTimeStep(0.25);&#xa;closeAllWindows();&#xa;runModel();" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" oldId="87" id="87">\n      <mxCell style="button;fontFamily=Garamond;fontSize=18;" parent="80" vertex="1">\n        <mxGeometry x="650" y="30" width="120" height="40" as="geometry"/>\n      </mxCell>\n    </Button>\n    <Setting Note="" Version="36" TimeLength="100" TimeStart="0" TimeStep="0.25" TimeUnits="Days" StrictUnits="true" StrictLinks="true" StrictAgentResolution="true" Units="" HiddenUIGroups="Validation,User Interface" SolutionAlgorithm="RK4" BackgroundColor="white" Throttle="-1" Macros="" SensitivityPrimitives="" SensitivityRuns="50" SensitivityBounds="50, 80, 95, 100" SensitivityShowRuns="false" article="{&quot;comments&quot;:true, &quot;facebookUID&quot;: &quot;&quot;}" StyleSheet="{}" unfolding="{&quot;text&quot;:&quot;Root Node&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;root&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Introduction&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:0,\\&quot;ids\\&quot;:[]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;84\\&quot;,\\&quot;85\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Welcome to&amp;nbsp;&lt;b&gt;&lt;i&gt;A Business Model&lt;/i&gt;&lt;/b&gt;!&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;In this model, we will investigate the impact of &lt;b&gt;delays &lt;/b&gt;on a &lt;b&gt;regulatory feedback system&lt;/b&gt; designed to maintain a given stock at a certain threshold. Specifically, we will focus on the example of a car dealership, where the stock is the car inventory, and a feedback mechanism is in place to regulate the inventory in response to changes in consumer demand.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;This story is structured in the following manner:&lt;/div&gt;&lt;div&gt;&lt;span style=\\&quot;white-space: pre;\\&quot;&gt;\\t&lt;/span&gt;1. Introduction&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;span style=\\&quot;white-space:pre\\&quot;&gt;\\t&lt;/span&gt;2. A Perfect System with No Delays&lt;/div&gt;&lt;div&gt;&lt;span style=\\&quot;white-space:pre\\&quot;&gt;\\t&lt;/span&gt;3. A Real System with Delays&lt;/div&gt;&lt;div&gt;&lt;span style=\\&quot;white-space:pre\\&quot;&gt;\\t&lt;/span&gt;4. Shortening Delays&lt;/div&gt;&lt;div&gt;&lt;span style=\\&quot;white-space:pre\\&quot;&gt;\\t&lt;/span&gt;5. Lengthening Delays&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;*Throughout the model, there will be some &lt;span style=\\&quot;color:grey\\&quot;&gt;&lt;i&gt;notes&lt;/i&gt;&amp;nbsp;&lt;/span&gt;that explain some of the implementation.&amp;nbsp;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Model Introduction&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;55\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Let us begin by considering a basic business inventory.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;In systems thinking, an &lt;b&gt;inventory &lt;/b&gt;can be represented as a &lt;b&gt;stock&lt;/b&gt;.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;For the purposes of this model, this stock will represent the &lt;b&gt;inventory of cars on the lot &lt;/b&gt;of a car dealership.&amp;nbsp;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Model Introduction 1&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;57\\&quot;,\\&quot;56\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;The inventory is affected by two &lt;b&gt;flows &lt;/b&gt;- an &lt;b&gt;inflow of deliveries&lt;/b&gt;, and an &lt;b&gt;outflow of sales&lt;/b&gt;.&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Model Introduction 2&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:0,\\&quot;ids\\&quot;:[\\&quot;80\\&quot;,\\&quot;78\\&quot;,\\&quot;81\\&quot;,\\&quot;82\\&quot;,\\&quot;83\\&quot;,\\&quot;87\\&quot;,\\&quot;72\\&quot;,\\&quot;74\\&quot;,\\&quot;73\\&quot;,\\&quot;75\\&quot;,\\&quot;77\\&quot;,\\&quot;76\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Now, consider a &lt;b&gt;regulatory feedback system&lt;/b&gt; designed in a such a manner so as to ensure the inventory is always able to cover &lt;b&gt;10 days of sales&lt;/b&gt;.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=\\&quot;color:grey\\&quot;&gt;&lt;i&gt;Therefore, the desired inventory is 10 * [perceived sales]; perceived sales is simply equal to sales (for now).&amp;nbsp;&lt;/i&gt;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Model Introduction 2.1&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;This system, although it looks more complicated, is very similar to the thermostat model described earlier in Chapter 2 of Meadows.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;It has two competing &lt;b&gt;balancing&lt;/b&gt;&lt;b&gt;&amp;nbsp;feedback loops&lt;/b&gt;, which are also known as &lt;i&gt;negative &lt;/i&gt;feedback loops.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;One balancing loop drains the inventory through the sales flow, while the other one maintains the inventory by affecting deliveries.&amp;nbsp;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 30&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;80\\&quot;,\\&quot;78\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Figure 30 shows the unsurprising result of a &lt;b&gt;10% increase&lt;/b&gt; in consumer demand.&amp;nbsp;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 30 - Run&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;pressButton(findName(\\&quot;Figure 30\\&quot;));&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;At the 25 day mark, the consumer demand is increased by 10%.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=\\&quot;color:grey\\&quot;&gt;&lt;i&gt;The consumer demand was originally set to 1, and was increased to 1.1 at time = 25 days. This was done using an IfThenElse statement.&amp;nbsp;&lt;/i&gt;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Delays&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;The simulation you just saw illustrates what might happen if there were &lt;b&gt;no delays&lt;/b&gt; in the system; if it were able to react instantaneously to the change in consumer demand.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;However, reality is not nearly as clear-cut and straightforward...&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Delays&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;In the real world, changes and decision do not happen instantaneously. There are many delays in the system that can slow down the regulatory feedback loop.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Three delays have been added to the model to better reflect reality: a &lt;b&gt;perception delay&lt;/b&gt;, a &lt;b&gt;response delay&lt;/b&gt; and a &lt;b&gt;delivery delay&lt;/b&gt;.&amp;nbsp;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Delays - Perception&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;74\\&quot;,\\&quot;76\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;The &lt;i&gt;perception delay &lt;/i&gt;is intentional, designed so that the dealer does not react to just any day-to-day changes in consumer demand. Before the dealer makes an order, she will wait&amp;nbsp;&lt;b&gt;5 days&lt;/b&gt;&amp;nbsp;and average the number of sales across those days to avoid making orders on the basis of short trends.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=\\&quot;color:grey\\&quot;&gt;&lt;i&gt;Perception delay was set to 5 days.&lt;/i&gt;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Delays - Response&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;72\\&quot;,\\&quot;77\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;&lt;div&gt;Even when it is clear that an order needs to be made, the dealer will not make the order in one go; rather, she will break up the order across&amp;nbsp;&lt;b&gt;3 days&lt;/b&gt;&amp;nbsp;to make absolutely sure the trend is real. This is represented in the model as the&amp;nbsp;&lt;i&gt;response delay&lt;/i&gt;.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;i style=\\&quot;color: rgb(128, 128, 128);\\&quot;&gt;Response delay was set to 3 days.&lt;/i&gt;&lt;br&gt;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Delays - Delivery&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;73\\&quot;,\\&quot;75\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Lastly, there is a &lt;i&gt;delivery delay&lt;/i&gt;. It takes &lt;b&gt;5 days&lt;/b&gt; for the dealer&#39;s order to be received, processed and the cars to be delivered to the dealership.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;i style=\\&quot;color: rgb(128, 128, 128);\\&quot;&gt;Delivery delay was set to 5 days.&lt;/i&gt;&lt;br&gt;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 32&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;81\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Figure 32 illustrates the effect of the delays on the system, with the same &lt;b&gt;10% increase&lt;/b&gt; in consumer demand as before.&amp;nbsp;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 32 - Run&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;pressButton(findName(\\&quot;Figure 32\\&quot;));&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;As you can see, there are &lt;b&gt;oscillations&lt;/b&gt;!&amp;nbsp;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 32 - Explained&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;&lt;i&gt;Why are there oscillations?&amp;nbsp;&lt;/i&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;1. The change in consumer demand causes &lt;b&gt;sales to go up&lt;/b&gt;, which in turn causes &lt;b&gt;inventory to drop&lt;/b&gt;.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;2. The dealer &lt;b&gt;waits &lt;/b&gt;to be sure that the higher demand is a &lt;b&gt;trend that will last&lt;/b&gt;, before making orders to bring the inventory up to cover the new sales rate.&amp;nbsp;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;3. However, in the time it takes for the orders to arrive, the &lt;b&gt;inventory drops furthe&lt;/b&gt;r. This causes the dealer to &lt;b&gt;bump up the orders&lt;/b&gt; so as to cover the desired 10 days worth of sales.&amp;nbsp;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 32 - Explained 2&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;&lt;div&gt;4. When the larger number of orders begins to arrive, the inventory recovers. In fact, the dealer has&amp;nbsp;&lt;b&gt;overcompensated&lt;/b&gt;; during the time spent waiting to make sure the trend was real,&amp;nbsp;&lt;b&gt;too many orders&amp;nbsp;were placed&lt;/b&gt;, and the inventory \\&quot;over-recovers.\\&quot;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;5. Realizing the error she made, the dealer &lt;b&gt;reduces the order number&lt;/b&gt;. Yet there are still large past orders being delivered, so she&amp;nbsp;&lt;b&gt;orders even less&lt;/b&gt;.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;6. Due to the uncertainty about the actual trend, it is almost inevitable that the dealer &lt;b&gt;orders too little&lt;/b&gt;. This again causes a drop in inventory and the cycle &lt;b&gt;repeats over and over&lt;/b&gt;&amp;nbsp;again, resulting in the &lt;b&gt;oscillations &lt;/b&gt;seen in Figure 32!&lt;/div&gt;&lt;div&gt;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 34&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;82\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;The dealer notices the oscillations and decides to do something about them.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Since she cannot do much about the delivery delay, she decides to &lt;b&gt;react faster herself&lt;/b&gt;.&amp;nbsp;&lt;/div&gt;&lt;div&gt;Instead of averaging sales trends over 5 days, she will only average them over 2 days.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=\\&quot;color:grey\\&quot;&gt;&lt;i&gt;Perception delay was set to 2 days.&amp;nbsp;&lt;/i&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Figure 34 shows what happens when the &lt;b&gt;perception delay is shortened to 2 days&lt;/b&gt;. Again, there is a 10% increase in customer demand as before.&amp;nbsp;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 34 - Run&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;pressButton(findName(\\&quot;Figure 34\\&quot;));&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;There is &lt;b&gt;not much of a difference&lt;/b&gt; in the oscillations! If anything, they are a little bit worse.&amp;nbsp;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 35&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;83\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;However, Figure 35 illustrates what happens if, instead of changing perception delay, the &lt;b&gt;response delay is shortened&lt;/b&gt; from 3 days to &lt;b&gt;2 days&lt;/b&gt;...&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=\\&quot;color:grey\\&quot;&gt;&lt;i&gt;Response delay was set to 2 days.&amp;nbsp;&lt;/i&gt;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 35 - Run&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;pressButton(findName(\\&quot;Figure 35\\&quot;));&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;The oscillations become &lt;b&gt;much worse&lt;/b&gt;!&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 36&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;87\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;The results of shortening delays, shown in Figures 34 and 35, show just how &lt;b&gt;surprising &lt;/b&gt;and &lt;b&gt;counter-intuitive &lt;/b&gt;systems can be when changes are forced upon them.&amp;nbsp;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;The issue in this case is that the car dealer has been reacting &lt;b&gt;too quickly&lt;/b&gt;, not too slowly.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Figure 36 illustrates the response in inventory when &lt;b&gt;response delay&lt;/b&gt; is increased to &lt;b&gt;6 days&lt;/b&gt; from 3 days.&amp;nbsp;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=\\&quot;color:grey\\&quot;&gt;&lt;i&gt;Response delay was set to 6 days.&amp;nbsp;&lt;/i&gt;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Figure 36 - Run&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;pressButton(findName(\\&quot;Figure 36\\&quot;));&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;The oscillations are &lt;b&gt;dampened considerably&lt;/b&gt;, and the system finds its equilibrium point very rapidly. &amp;nbsp;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Conclusion&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;: 100, \\&quot;ids\\&quot;: []}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;&lt;div&gt;As this model has illustrated, &lt;b&gt;changing the delays &lt;/b&gt;in a system can drastically &lt;b&gt;change the behaviour of the system&lt;/b&gt; as a whole. These changes can make the system more manageable, but it can also easily have the opposite effect. As such, the concept of delays is very important under the umbrella of systems thinking. Only when the delays in a system are understood can the &lt;b&gt;dynamic behaviour&lt;/b&gt; of that system be thoroughly investigated.&amp;nbsp;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Conclusion&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;&lt;div style=\\&quot;font-family:courier new; font-size:40px; color:darkgreen\\&quot;&gt;&lt;b&gt;The End!&lt;/b&gt;&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;Thanks for reading! :)&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div style=\\&quot;color: grey;\\&quot;&gt;&lt;i&gt;*Acknowledgements can be found under the acknowledgements section in the mode&lt;/i&gt;l.&amp;nbsp;&lt;/div&gt;&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]}]}" unfoldingStatus="on" unfoldingAuto="non-editors" id="2">\n      <mxCell parent="1" vertex="1" visible="0">\n        <mxGeometry x="20" y="20" width="80" height="40" as="geometry"/>\n      </mxCell>\n    </Setting>\n    <Display name="Default Display" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="Cars on the Lot" ThreeDimensional="false" Primitives="55" AutoAddPrimitives="true" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" yAxis2="" Primitives2="" showMarkers="false" showLines="true" showArea="false" legendPosition="None" yAxisMin="0" yAxisMax="500" id="3">\n      <mxCell style="roundImage;image=/builder/images/DisplayFull.png;" parent="1" vertex="1" visible="0">\n        <mxGeometry x="50" y="20" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Display>\n    <Stock name="inventory of cars on the lot" Note="" InitialValue="200" StockMode="Store" Delay="10" Volume="100" NonNegative="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" AllowNegatives="false" id="55">\n      <mxCell style="stock;fontFamily=Garamond;fontSize=24;rounded=1;fillColor=#f6ffc5;" parent="1" vertex="1">\n        <mxGeometry x="384" y="113" width="186" height="80" as="geometry"/>\n      </mxCell>\n    </Stock>\n    <Flow name="sales" Note="" FlowRate="20 * [customer demand]" OnlyPositive="true" TimeIndependent="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" id="56">\n      <mxCell style="flow;fontFamily=Garamond;fontSize=20;strokeColor=#339966;fontColor=#003366;strokeWidth=6;30=60;" parent="1" source="55" edge="1">\n        <mxGeometry x="16" y="-197" width="100" height="100" as="geometry">\n          <mxPoint x="16" y="-97" as="sourcePoint"/>\n          <mxPoint x="860" y="153" as="targetPoint"/>\n          <mxPoint x="-5" y="-20" as="offset"/>\n        </mxGeometry>\n      </mxCell>\n    </Flow>\n    <Flow name="deliveries" Note="" FlowRate="IfThenElse(Time() &gt; {25 Days}, Delay1([orders to factory], [delivery delay]), [orders to factory])" OnlyPositive="true" TimeIndependent="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" id="57">\n      <mxCell style="flow;fontFamily=Garamond;fontSize=20;strokeColor=#339966;fontColor=#003366;strokeWidth=6;30=60;" parent="1" target="55" edge="1">\n        <mxGeometry x="16" y="-197" width="100" height="100" as="geometry">\n          <mxPoint x="80" y="153" as="sourcePoint"/>\n          <mxPoint x="16" y="-97" as="targetPoint"/>\n          <Array as="points"/>\n          <mxPoint y="-20" as="offset"/>\n        </mxGeometry>\n      </mxCell>\n    </Flow>\n    <Variable name="orders to factory" Note="" Equation="IfThenElse(Time() &gt; {25 Days}, Delay3([discrepancy], [response delay]) + Delay3([perceived sales], [response delay]), [discrepancy] + [perceived sales])" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" oldId="58" id="58">\n      <mxCell style="variable;fontFamily=Garamond;fontSize=18;fillColor=#d6ffc7;" parent="1" vertex="1">\n        <mxGeometry x="174" y="313" width="130" height="60" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Variable name="discrepancy" Note="" Equation="[desired inventory]-[inventory of cars on the lot]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" oldId="59" id="59">\n      <mxCell style="variable;fontFamily=Garamond;fontSize=18;fillColor=#d6ffc7;" parent="1" vertex="1">\n        <mxGeometry x="330" y="293" width="120" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Variable name="desired inventory" Note="" Equation="10 * [perceived sales]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" oldId="60" id="60">\n      <mxCell style="variable;fontFamily=Garamond;fontSize=18;fillColor=#d6ffc7;" parent="1" vertex="1">\n        <mxGeometry x="431" y="373" width="120" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Variable name="perceived sales" Note="" Equation="IfThenElse(Time() &gt; {25 Days}, Delay3([sales], [perception delay]), [sales])" Units="