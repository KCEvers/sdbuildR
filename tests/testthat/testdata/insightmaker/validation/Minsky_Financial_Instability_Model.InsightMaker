lse" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="107">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="45" y="446.5" width="75" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="108">\n      <mxCell style="link" parent="1" source="106" target="107" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint"/>\n          <mxPoint x="100" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="109">\n      <mxCell style="link" parent="1" source="73" target="107" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint"/>\n          <mxPoint x="100" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="110">\n      <mxCell style="link" parent="1" source="88" target="96" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint"/>\n          <mxPoint x="100" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Picture name="Notes" Note="&lt;span style=&quot;color: rgb(31, 73, 125); font-family: Calibri, sans-serif; font-size: 15px;&quot;&gt;I believe the Minsky model needs&amp;nbsp;&lt;/span&gt;&lt;a href=&quot;http://insightmaker.com/insight/3023&quot; target=&quot;_blank&quot;&gt;sectors&lt;/a&gt;&lt;span style=&quot;color: rgb(31, 73, 125); font-family: Calibri, sans-serif; font-size: 15px;&quot;&gt;&amp;nbsp;to be useful for policy advice about public austerity vs public deficit. Hence my suggestion to try to integrate them (initially by adding sectors to the MInsky model)â¦&lt;/span&gt;&lt;div&gt;&lt;span style=&quot;color: rgb(31, 73, 125); font-family: Calibri, sans-serif; font-size: 15px;&quot;&gt;&lt;br&gt;&lt;/span&gt;&lt;/div&gt;&lt;div&gt;&lt;p class=&quot;MsoNormal&quot; style=&quot;margin: 0cm 0cm 0.0001pt; font-size: 12pt; font-family: &#39;Times New Roman&#39;, serif;&quot;&gt;&lt;span style=&quot;font-size: 11pt; font-family: Calibri, sans-serif; color: rgb(31, 73, 125);&quot;&gt;Your thoughts on what you require from a good model, where to start and what interventions to focus on would be extremely helpful, Lori.&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;color: rgb(31, 73, 125); font-family: Calibri, sans-serif; font-size: 11pt;&quot;&gt;For instance how would you split output in IM 2011 to conform with MMT sectors(Govt Private External?)&amp;nbsp;&amp;nbsp; and where would banks and money fit without being too complicated?&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;color: rgb(31, 73, 125); font-family: Calibri, sans-serif; font-size: 11pt;&quot;&gt;Or should we just start all over again?&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;MsoNormal&quot; style=&quot;margin: 0cm 0cm 0.0001pt; font-size: 12pt; font-family: &#39;Times New Roman&#39;, serif;&quot;&gt;&lt;span style=&quot;font-size: 11pt; font-family: Calibri, sans-serif; color: rgb(31, 73, 125);&quot;&gt;Would a broad or narrow discussion thread focussed on building a model that meets your/our requirements help right now?&lt;/span&gt;&lt;/p&gt;&lt;p class=&quot;MsoNormal&quot; style=&quot;margin: 0cm 0cm 0.0001pt; font-size: 12pt; font-family: &#39;Times New Roman&#39;, serif;&quot;&gt;&lt;span style=&quot;font-size: 11pt; font-family: Calibri, sans-serif; color: rgb(31, 73, 125);&quot;&gt;See also&amp;nbsp;&lt;/span&gt;&lt;a href=&quot;http://andrewlainton.wordpress.com/2014/05/25/at-the-top-of-an-overheating-housing-market-relaxing-planning-can-make-matters-much-worse/&quot; target=&quot;_blank&quot;&gt;housing post&lt;/a&gt;&lt;span style=&quot;font-size: 11pt; font-family: Calibri, sans-serif; color: rgb(31, 73, 125);&quot;&gt;&amp;nbsp;at Andrew Laintoin&#39;s blog&lt;/span&gt;&lt;/p&gt;&lt;/div&gt;" Image="https://res.cloudinary.com/sysnet/image/upload/t_media_lib_thumb/v1390377803/note.png" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" id="112">\n      <mxCell style="picture;image=https://res.cloudinary.com/sysnet/image/upload/t_media_lib_thumb/v1390377803/note.png;imageFlipV=0;imageFlipH=0;shape=image" parent="1" vertex="1">\n        <mxGeometry x="730" y="5" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Picture>\n  </root>\n</insightmakermodel>
