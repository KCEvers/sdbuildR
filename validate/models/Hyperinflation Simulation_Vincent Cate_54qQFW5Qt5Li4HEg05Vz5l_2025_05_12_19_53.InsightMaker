<insightmakermodel>\n  <root> \n<header> model_id="54qQFW5Qt5Li4HEg05Vz5l", model_title="Hyperinflation Simulation", model_author_id="6jsk0VFtFhgrIe2jLqJpLN", model_author_name="Vincent Cate" </header>\n    <mxCell id="0"/>\n    <mxCell id="1" parent="0"/>\n    <Setting Note="" Version="36" TimeLength="3650" TimeStart="0" TimeStep="1" TimeUnits="Days" StrictUnits="true" Units="" HiddenUIGroups="Validation,User Interface" SolutionAlgorithm="RK1" BackgroundColor="white" Throttle="-1" Macros="" SensitivityPrimitives="" SensitivityRuns="50" SensitivityBounds="50, 80, 95, 100" SensitivityShowRuns="false" article="{&quot;comments&quot;:true, &quot;facebookUID&quot;: &quot;&quot;}" StyleSheet="{}" id="2">\n      <mxCell parent="1" vertex="1" visible="0">\n        <mxGeometry x="20" y="20" width="80" height="40" as="geometry"/>\n      </mxCell>\n    </Setting>\n    <Stock name="Money Supply" Note="" InitialValue="# Money supply here should be thought of as money outside the Fed or government\n# You can imagine that the government burns all money collected from taxes or bond sales and prints all money used for spending or paying off bonds.\n\n# We are doing Fed balance sheet of 4 trillion\n\n# We start with \n#T  B  M  T\n4000000000000" StockMode="Store" Delay="10" Volume="100" NonNegative="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="1000000000000000000" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="23">\n      <mxCell style="stock" parent="1" vertex="1">\n        <mxGeometry x="405" y="226.56305882915433" width="100" height="50" as="geometry"/>\n      </mxCell>\n    </Stock>\n    <Stock name="Government " Note="" InitialValue="# This stock is debt from deficit spending\n# Does not account for debt from bond redemptions\n# This starts at national debt \n# Note this stock can go negative and probably always is\n-16000000000000" StockMode="Store" Delay="10" Volume="100" NonNegative="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="25">\n      <mxCell style="stock" parent="1" vertex="1">\n        <mxGeometry x="405" y="6.563058829154329" width="110" height="100" as="geometry"/>\n      </mxCell>\n    </Stock>\n    <Stock name="Private Bond Holdings plus excess reserves" Note="" InitialValue="20047393364929" StockMode="Store" Delay="10" Volume="100" NonNegative="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="true" SliderMax="30000000000000" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" AllowNegatives="false" id="29">\n      <mxCell style="stock" parent="1" vertex="1">\n        <mxGeometry x="110" y="154.06305882915433" width="110" height="112.5" as="geometry"/>\n      </mxCell>\n    </Stock>\n    <Variable name="Velocity of Money" Note="" Equation="# See Hussman on how interest rate influences velocity of money\n# http://www.hussmanfunds.com/wmc/wmc110124.htm\n# In hyperinflation the inflation rate \n#  also influences velocity\n# Note that when Fed forces interest\n#  down it lowers velocity, at first\n# Limit velocity to once per day maybe \n# should be 2 or 3 times per day, but humans\n# only move so fast, so some limit reasonable\nMin(365,Max(Abs([Yearly Inflation stock]), Abs([Yearly Interest Factor])))" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="30">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="505" y="436.56305882915433" width="120" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Variable name="Price Level" Note="" Equation="# This uses the &quot;equation of exchange&quot; and solves for the price level.\n# http://en.wikipedia.org/wiki/Equation_of_exchange\n# This is key for understanding the math of hyperinflation.  Look at:\n# http://www.sjsu.edu/faculty/watkins/infldynamic.htm\n\n# If you get a &quot;error, not a number, did you\n#   divide by zero?&quot; the problem is the\n#   money supply is so large that\n#   multiplying by velocity exceeds max num\n\n[Money Supply]*[Velocity of Money]/[Real GNP]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="31">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="730" y="451.56305882915433" width="120" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Variable name="Real GNP" Note="" Equation="# for low levels of inflation we use initial real GNP\n# for early hyperinflation we reduce GNP by inflation factor over 1.3\n# for high hyperinflation we reduce GNP by fixed factor of 2.3\nIfThenElse([Yearly Inflation stock]&lt;1.3, [Initial Real GNP], IfThenElse([Yearly Inflation stock]&gt;3, [Initial Real GNP]/2.3, [Initial Real GNP]/([Yearly Inflation stock]/1.3)))\n#[Initial Real GNP]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="17000000000000" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="32">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="650" y="306.56305882915433" width="120" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Flow name="Daily Federal Deficit" Note="" FlowRate="# Positive then money supply is increasing\n# Imagine they burn money from taxes and print all money they spend\n# Then deficit is net increase in money\nIfThenElse(Days&lt;5, 0, [Daily Spending]-[Daily Taxes])" OnlyPositive="false" TimeIndependent="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" id="37">\n      <mxCell style="flow;startArrow=block;startFill=0;" parent="1" source="25" target="23" edge="1">\n        <mxGeometry x="195" y="-8.436941170845671" width="100" height="100" as="geometry">\n          <mxPoint x="195" y="91.56305882915433" as="sourcePoint"/>\n          <mxPoint x="450" y="206.56305882915433" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Flow>\n    <Variable name="Tax Fraction of GNP" Note="" Equation="0.327" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="true" SliderMax="1" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="38">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="750" y="16.56305882915433" width="170" height="65" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Variable name="Nominal GNP" Note="" Equation="[Real GNP]*[Price Level]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="39">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="810" y="219.06305882915433" width="120" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="41">\n      <mxCell style="link" parent="1" source="38" target="130" edge="1">\n        <mxGeometry x="105" y="-6.2281641014907905" width="100" height="100" as="geometry">\n          <mxPoint x="105" y="93.77183589850921" as="sourcePoint"/>\n          <mxPoint x="460" y="166.56305882915433" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="42">\n      <mxCell style="link" parent="1" source="39" target="130" edge="1">\n        <mxGeometry x="105" y="30.018882169856113" width="100" height="100" as="geometry">\n          <mxPoint x="105" y="130.0188821698561" as="sourcePoint"/>\n          <mxPoint x="560" y="176.56305882915433" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="Bond Goodness Factor" Note="" Equation="#  People get paid interest on their bonds.\n# This increases the money supply.\n#  For this simulation that is just part of the government budget. See vince cate CMMT\n\n# Idea in this formula is that people \n#   will increase bond holdings if the \n#   interest is higher than inflation and\n#   decrease if is less than inflation.\n# People fleeing bonts is a trigger for hyperinflation\n# Goodness of bonds \n# The Bonus is because it seems people buy them even if inflation rate is a bit higher than interest rate\n[Bond Reputation Bonus] + [Yearly Interest Factor]-[Yearly Inflation stock]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="46">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="80" y="392" width="140" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Flow name="Net Bond Redemptions" Note="" FlowRate="# You can imagine that the government burns all money from bond sales and prints all money for bond redemptions\n# So positive net redemptions increases money supply and negative reduces it\n\nIfThenElse(Days&lt;30, 0, [Private Bond Holdings plus excess reserves]*[Daily Bond Exit Rate] - [Daily Bond Buying Rate]*[Money Supply])\n" OnlyPositive="false" TimeIndependent="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" id="61">\n      <mxCell style="flow;startArrow=block;startFill=0;" parent="1" source="29" target="23" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="350" y="256.56305882915433" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Flow>\n    <Link name="Link" Note="" BiDirectional="false" id="63">\n      <mxCell style="link" parent="1" source="23" target="31" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="64">\n      <mxCell style="link;" parent="1" source="30" target="31" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="67">\n      <mxCell style="link" parent="1" source="32" target="39" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="68">\n      <mxCell style="link" parent="1" source="31" target="39" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="69">\n      <mxCell style="link" parent="1" source="32" target="31" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="Yearly Interest Rate %" Note="" Equation="7.77" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="true" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="71">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="260" y="332" width="130" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Variable name="Daily Bond Exit Rate" Note="" Equation="# Fraction of bond holders selling today\n# Note we assume central bank always keeps \n#  a bid since government fails if it can\n#  not sell bonds.\n\n# Negative of Goodness for how bad and how \n#   much people will be exiting\n\n\nIfThenElse([Bond Goodness Factor]&gt;0, 0, IfThenElse([Bond Goodness Factor]&lt; -1, 0.01, [Bond Goodness Factor] * -0.01))\n    " Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="76">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="75" y="292" width="150" height="58" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="77">\n      <mxCell style="link" parent="1" source="46" target="76" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="78">\n      <mxCell style="link" parent="1" source="76" target="61" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Display name="Yearly Inflation Factor" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="" yAxis2="" showMarkers="false" showLines="true" showArea="false" ThreeDimensional="false" Primitives="127" Primitives2="" AutoAddPrimitives="false" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" legendPosition="Automatic" id="88">\n      <mxCell style="display" parent="1" vertex="1" visible="0">\n        <mxGeometry x="10" y="10" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Display>\n    <Display name="Bond Holdings" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="" yAxis2="" showMarkers="false" showLines="true" showArea="false" ThreeDimensional="false" Primitives="29" Primitives2="" AutoAddPrimitives="false" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" legendPosition="Automatic" id="90">\n      <mxCell style="display" parent="1" vertex="1" visible="0">\n        <mxGeometry x="10" y="10" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Display>\n    <Display name="Bond Redemptions" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="" yAxis2="" showMarkers="false" showLines="true" showArea="false" ThreeDimensional="false" Primitives="61" Primitives2="" AutoAddPrimitives="false" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" legendPosition="Automatic" id="94">\n      <mxCell style="display" parent="1" vertex="1" visible="0">\n        <mxGeometry x="10" y="10" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Display>\n    <Display name="Daily Bond Exit Rate" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="" yAxis2="" showMarkers="false" showLines="true" showArea="false" ThreeDimensional="false" Primitives="76" Primitives2="" AutoAddPrimitives="false" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" legendPosition="Automatic" id="95">\n      <mxCell style="display" parent="1" vertex="1" visible="0">\n        <mxGeometry x="10" y="10" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Display>\n    <Variable name="Yearly Interest Factor" Note="" Equation="IfThenElse(days&lt;[Days to 0 interest],1+([Yearly Interest Rate %]+days*[Change in Interest])/100, 1)" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="97">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="250" y="402" width="130" height="58" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="98">\n      <mxCell style="link" parent="1" source="71" target="97" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="99">\n      <mxCell style="link" parent="1" source="97" target="46" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="Initial Real GNP" Note="" Equation="20000000000000" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="20000000000000" MinConstraint="0" ShowSlider="true" SliderMax="30000000000000" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="117">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="570" y="222" width="140" height="50" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="118">\n      <mxCell style="link" parent="1" source="117" target="32" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="121">\n      <mxCell style="link" parent="1" source="97" target="30" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Display name="Big Table" Note="" Type="Tabular" xAxis="Time (%u)" yAxis="" yAxis2="" showMarkers="false" showLines="true" showArea="false" ThreeDimensional="false" Primitives="30,23,31,97,46,32,29,25,37,127" Primitives2="" AutoAddPrimitives="false" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" legendPosition="Automatic" id="124">\n      <mxCell style="display" parent="1" vertex="1" visible="0">\n        <mxGeometry x="10" y="10" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Display>\n    <Display name="Price Level" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="" yAxis2="" showMarkers="false" showLines="true" showArea="false" ThreeDimensional="false" Primitives="31" Primitives2="" AutoAddPrimitives="false" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" legendPosition="Automatic" id="125">\n      <mxCell style="display" parent="1" vertex="1" visible="0">\n        <mxGeometry x="10" y="10" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Display>\n    <Display name="Money Supply" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="" yAxis2="" showMarkers="false" showLines="true" showArea="false" ThreeDimensional="false" Primitives="23" Primitives2="" AutoAddPrimitives="false" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" legendPosition="Automatic" id="126">\n      <mxCell style="display" parent="1" vertex="1" visible="0">\n        <mxGeometry x="10" y="10" width="64" height="64" as="geometry"/>\n      </mxCell>\n    </Display>\n    <Variable name="Yearly Inflation factor" Note="" Equation="#  First 365 days we are doing price level compared\n# to price level &quot;days&quot; back then just 365 days back\n#  So 2% inflation per year returns 1.02  \nIfThenElse(days&lt;366,[Price Level]/Delay([Price Level], days, 1.02),[Price Level]/delay([Price Level],365,1.02))" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="127">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="400" y="673" width="210" height="75" as="geometry"/>\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="128">\n      <mxCell style="link" parent="1" source="31" target="127" edge="1">\n        <mxGeometry x="100" y="6.563058829154329" width="100" height="100" as="geometry">\n          <mxPoint x="100" y="106.56305882915433" as="sourcePoint"/>\n          <mxPoint x="200" y="6.563058829154329" as="targetPoint"/>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="Daily Taxes" Note="" Equation="#  We assume taxes are a fixed percentage of\n#    Nominal GNP but that it takes 30 days \n#    to collect these so we use GNP of 30\n#    days ago\n# In high inflation that is much smaller \n#    than current GNP\n#  Really it is probably more than 30 days\n#     but this is to show the idea not\n#     exactly model a real country\n#  In real hyperinflation much of the\n#   economy goes black market to avoid taxes\n# So really it is worse than this simulation\n# Taxes are collected after a delay so need to \n#   look back and Nominal GNP then\n\nifThenElse(Days&lt;1+[Days to collect taxes],[Tax Fraction of GNP]*[Nominal GNP]/365, [Tax Fraction of GNP]*Delay([Nominal GNP], [Days to collect taxes], 0)/365)" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="130">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="550" y="81.56305882915433" width="1