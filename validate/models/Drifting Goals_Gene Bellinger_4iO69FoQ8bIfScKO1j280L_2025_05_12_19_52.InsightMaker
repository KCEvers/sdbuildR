<insightmakermodel>\n  <root> \n<header> model_id="4iO69FoQ8bIfScKO1j280L", model_title="Drifting Goals", model_author_id="6jMuKMJEK1DPp1heQgk4sd", model_author_name="Gene Bellinger" </header>\n    <mxCell id="0" />\n    <mxCell id="1" parent="0" />\n    <Setting Note="" Version="37" TimeLength="20" TimeStart="0" TimeStep="0.5" TimeUnits="Weeks" Units="" HiddenUIGroups="Validation,User Interface" SolutionAlgorithm="RK1" BackgroundColor="white" Throttle="1" Macros="" SensitivityPrimitives="" SensitivityRuns="50" SensitivityBounds="50, 80, 95, 100" SensitivityShowRuns="false" StyleSheet="{&quot;Link&quot;:{&quot;30&quot;:&quot;50&quot;,&quot;shape&quot;:&quot;connector&quot;,&quot;endArrow&quot;:&quot;classic&quot;,&quot;verticalAlign&quot;:&quot;middle&quot;,&quot;align&quot;:&quot;center&quot;,&quot;strokeColor&quot;:&quot;#0000FF&quot;,&quot;fontColor&quot;:&quot;#808080&quot;,&quot;labelBackgroundColor&quot;:&quot;white&quot;,&quot;rounded&quot;:1,&quot;fontSize&quot;:14,&quot;edgeStyle&quot;:&quot;straight&quot;,&quot;fontFamily&quot;:&quot;Comic Sans MS&quot;,&quot;strokeWidth&quot;:&quot;5&quot;,&quot;opacity&quot;:70,&quot;curved&quot;:1,&quot;dashed&quot;:&quot;0&quot;,&quot;noLabel&quot;:0}}" oldId="undefined" unfolding="{&quot;text&quot;:&quot;Root Node&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;root&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Setup&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:false,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;topBarShown()?toggleTopBar():0;\\r\nsideBarShown()?toggleSideBar():0;\\r\nsetZoom(\\&quot;fit\\&quot;);\\r\n&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:0,\\&quot;ids\\&quot;:[]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;94\\&quot;,\\&quot;70\\&quot;,\\&quot;57\\&quot;,\\&quot;68\\&quot;,\\&quot;58\\&quot;,\\&quot;60\\&quot;,\\&quot;59\\&quot;,\\&quot;66\\&quot;,\\&quot;69\\&quot;,\\&quot;67\\&quot;,\\&quot;61\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;This structure begins with a standard &lt;b&gt;Goal Seeking&lt;/b&gt; structure which attempt to move [&lt;b&gt;what i have&lt;/b&gt;] to the desired state [&lt;b&gt;what i want&lt;/b&gt;] via the [&lt;b&gt;action to get what i want&lt;/b&gt;].&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;B2 Settle for Less&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:false,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:25,\\&quot;ids\\&quot;:[\\&quot;61\\&quot;,\\&quot;67\\&quot;,\\&quot;69\\&quot;,\\&quot;66\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;68\\&quot;,\\&quot;57\\&quot;,\\&quot;71\\&quot;,\\&quot;74\\&quot;,\\&quot;72\\&quot;,\\&quot;73\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Though there&#39;s a difficulty that often arises because of the &lt;b&gt;delay&lt;/b&gt; associated with getting from [&lt;b&gt;action to get what i want&lt;/b&gt;] moving [&lt;b&gt;what i have&lt;/b&gt;] to [&lt;b&gt;what i want&lt;/b&gt;]. The &lt;b&gt;Goal Seeking loop B2&lt;/b&gt; is also capable of relieving the tension which is the [&lt;b&gt;gap&lt;/b&gt;].&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Conclusion&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;67\\&quot;,\\&quot;69\\&quot;,\\&quot;66\\&quot;,\\&quot;61\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;If the [&lt;b&gt;pressure to settle for less&lt;/b&gt;] is great enough so it reduces [&lt;b&gt;what i want&lt;/b&gt;] then the [&lt;b&gt;gap&lt;/b&gt;] is reduced. Which is why this is called a &lt;b&gt;Drifting Goals&lt;/b&gt; structure. There are two ways to relieve the tension and one is generally easier and takes less time than the other.&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Simulation&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:false,&quot;children&quot;:[{&quot;text&quot;:&quot;Visibility Change&quot;,&quot;data&quot;:&quot;{\\&quot;opacity\\&quot;:100,\\&quot;ids\\&quot;:[\\&quot;79\\&quot;,\\&quot;80\\&quot;,\\&quot;81\\&quot;,\\&quot;82\\&quot;,\\&quot;83\\&quot;,\\&quot;84\\&quot;,\\&quot;85\\&quot;,\\&quot;86\\&quot;,\\&quot;87\\&quot;,\\&quot;88\\&quot;,\\&quot;89\\&quot;,\\&quot;90\\&quot;,\\&quot;91\\&quot;,\\&quot;92\\&quot;,\\&quot;93\\&quot;,\\&quot;95\\&quot;]}&quot;,&quot;type&quot;:&quot;visibility&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;The equivalent Stock &amp;amp; Flow structure adds [&lt;b&gt;pct chg fact&lt;/b&gt;] to govern the extent to which [&lt;b&gt;tension&lt;/b&gt;] affects [&lt;b&gt;action&lt;/b&gt;] and [&lt;b&gt;goal chg fact&lt;/b&gt;] to govern the extent to which [&lt;b&gt;tension&lt;/b&gt;] affects [&lt;b&gt;goal reduction&lt;/b&gt;].&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Simulation gcf = 0&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:false,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;setTimeStart(0);\\r\nsetTimeLength(20);\\r\nsetTimeStep(0.5);\\r\nsetTimeUnits(\\&quot;Weeks\\&quot;);\\r\nsetPauseInterval(0);\\r\nsetAlgorithm(\\&quot;RK1\\&quot;);\\r\nsetValue(findName(\\&quot;desired state\\&quot;),1);\\r\nsetValue(findName(\\&quot;current state\\&quot;),0);\\r\nsetValue(findName(\\&quot;pct chg fact\\&quot;),0.2);\\r\nsetValue(findName(\\&quot;goal chg fact\\&quot;),0);\\r\nupdateSideBar();\\r\n&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Run Simulation&quot;,&quot;data&quot;:&quot;{}&quot;,&quot;type&quot;:&quot;simulate&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Running the simulation with [&lt;b&gt;desired state&lt;/b&gt;] = 1, [&lt;b&gt;current state&lt;/b&gt;] = 0, [&lt;b&gt;pct chg fact&lt;/b&gt;] = 0.2 and [&lt;b&gt;goal chg fact&lt;/b&gt;] = 0 we get the typical goal seeking structure behavior though it takes almost 20 weeks to reach the goal. This may be perceived as taking way too long and cause frustration.&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;simulation gcf = 0.05&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:false,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();\\r\nsetValue(findName(\\&quot;goal chg fact\\&quot;),0.05);\\r\nupdateSideBar();&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Run Simulation&quot;,&quot;data&quot;:&quot;{}&quot;,&quot;type&quot;:&quot;simulate&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;If we change the value of [&lt;b&gt;goal chg fact&lt;/b&gt;] = 0.05 we notice a marked reduction in the [&lt;b&gt;desired state&lt;/b&gt;] and hence it doesn&#39;t take nearly as long to reach the goal.&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;simulation pcf = 0.5&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:false,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();\\r\nsetValue(findName(\\&quot;pct chg fact\\&quot;),0.5);\\r\nupdateSideBar();&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Run Simulation&quot;,&quot;data&quot;:&quot;{}&quot;,&quot;type&quot;:&quot;simulate&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;If we set [&lt;b&gt;pct chg fact&lt;/b&gt;] = 0.5 and run the simulation we note that it takes much less time to reach the [&lt;b&gt;desired state&lt;/b&gt;] and it declines far less.&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;It should be apparent that the the extent the goal drifts is dependent both the time it takes to move in the desired direction and the willingness to deal with the tension created by not yet being there.&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Strategy&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:true,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();\\r\n&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;&lt;b&gt;Strategy&lt;/b&gt;: The two effective strategies for dealing with this structure are: 1) disconnect the [&lt;b&gt;pressure to settle for less&lt;/b&gt;] from [&lt;b&gt;what i want&lt;/b&gt;] so it can&#39;t cause the goal to drift; 2) Increase the rate at which there is progress toward [&lt;b&gt;what i have&lt;/b&gt;] so there&#39;s less time for the goal to drift. The viability of either of these will depend on the actual elements in the structure.&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]},{&quot;text&quot;:&quot;Interact&quot;,&quot;data&quot;:&quot;&quot;,&quot;type&quot;:&quot;group&quot;,&quot;leaf&quot;:false,&quot;expanded&quot;:false,&quot;children&quot;:[{&quot;text&quot;:&quot;Run Action&quot;,&quot;data&quot;:&quot;closeAllWindows();\\r\ntopBarShown()?0:toggleTopBar();\\r\nsideBarShown()?0:toggleSideBar();\\r\nsetZoom(\\&quot;fit\\&quot;);\\r\nsetValue(findName(\\&quot;desired state\\&quot;),1);\\r\nsetValue(findName(\\&quot;current state\\&quot;),0);\\r\nsetValue(findName(\\&quot;pct chg fact\\&quot;),0.2);\\r\nsetValue(findName(\\&quot;goal chg fact\\&quot;),0);\\r\nupdateSideBar();&quot;,&quot;type&quot;:&quot;action&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]},{&quot;text&quot;:&quot;Show Message&quot;,&quot;data&quot;:&quot;Change the sliders to run the model with various parameters to see how they impact the behavior of the model. Also change the&amp;nbsp;&lt;b&gt;Simulation Time Step&lt;/b&gt;&amp;nbsp;in&amp;nbsp;&lt;b&gt;Settings&lt;/b&gt;&amp;nbsp;to 1, .5, .25, and .125 to see how this affects the nature of the graphs.&quot;,&quot;type&quot;:&quot;note&quot;,&quot;leaf&quot;:true,&quot;expanded&quot;:false,&quot;children&quot;:[]}]}]}" unfoldingStatus="on" unfoldingAuto="never" TimePause="0" id="2">\n      <mxCell parent="1" vertex="1" visible="0">\n        <mxGeometry x="20" y="20" width="80" height="40" as="geometry" />\n      </mxCell>\n    </Setting>\n    <Display name="Default Display" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="%o" ThreeDimensional="false" Primitives="79,82,84,83,80" AutoAddPrimitives="true" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" yAxis2="" Primitives2="" showMarkers="false" showLines="true" showArea="false" legendPosition="Automatic" oldId="undefined" id="3">\n      <mxCell style="roundImage;image=/builder/images/DisplayFull.png;" parent="1" vertex="1" visible="0">\n        <mxGeometry x="50" y="20" width="64" height="64" as="geometry" />\n      </mxCell>\n    </Display>\n    <Folder name="" Note="&lt;p&gt;Drifting Goals Systems Archetype CLD.&lt;/p&gt;" Type="None" Solver="{&quot;enabled&quot;: false, &quot;algorithm&quot;: &quot;RK1&quot;, &quot;timeStep&quot;: 1}" Image="Plugin" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" AgentBase="" oldId="undefined" id="55">\n      <mxCell style="folder;image=None;shape=rectangle;imageFlipV=0;imageFlipH=0;strokeColor=none;opacity=0;textOpacity=0" parent="1" vertex="1">\n        <mxGeometry x="16" y="94" width="620" height="372" as="geometry">\n          <mxRectangle x="50" y="152" width="128" height="128" as="alternateBounds" />\n        </mxGeometry>\n      </mxCell>\n    </Folder>\n    <Setting Note="" Version="34" TimeLength="100" TimeStart="0" TimeStep="1" TimeUnits="Years" Units="" HiddenUIGroups="Validation,User Interface" SolutionAlgorithm="RK1" BackgroundColor="white" Throttle="-1" Macros="" SensitivityPrimitives="" SensitivityRuns="50" SensitivityBounds="50, 80, 95, 100" SensitivityShowRuns="false" oldId="undefined" id="56">\n      <mxCell parent="55" vertex="1" visible="0">\n        <mxGeometry x="20" y="20" width="80" height="40" as="geometry" />\n      </mxCell>\n    </Setting>\n    <Variable name="what i want" Note="The objective or state of things I want to bring into being." Equation="0" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" LabelPosition="Middle" FlipHorizontal="false" FlipVertical="false" oldId="undefined" id="57">\n      <mxCell style="parameter;fillColor=none;labelBackgroundColor=none;strokeColor=none;opacity=100;textOpacity=100" parent="55" vertex="1">\n        <mxGeometry x="120" y="63.5" width="60" height="35" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Variable name="gap" Note="The difference  between what I want and what I have. The gap serves as a motivator for action." Equation="0" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" Image="None" LabelPosition="Middle" FlipHorizontal="false" FlipVertical="false" oldId="undefined" id="58">\n      <mxCell style="parameter;fillColor=none;labelBackgroundColor=n