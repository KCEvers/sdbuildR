<insightmakermodel>\n  <root> \n<header> model_id="6G3s10hbWkyECivkThcYX2", model_title="D-model (curve di Richards) con -ln(alpha)=lag*mu", model_author_id="2FHygdfmZGH0vAK6uRlRyl", model_author_name="Eugenio Parente" </header>\n    <mxCell id="0" />\n    <mxCell id="1" parent="0" />\n    <Setting Note="" Version="37" TimeLength="24" TimeStart="0" TimeStep="0.1" TimeUnits="Hours" Units="" HiddenUIGroups="Validation,User Interface" SolutionAlgorithm="RK4" BackgroundColor="white" Throttle="10" Macros="" SensitivityPrimitives="" SensitivityRuns="50" SensitivityBounds="50, 80, 95, 100" SensitivityShowRuns="false" StyleSheet="{}" id="2">\n      <mxCell parent="1" vertex="1" visible="0">\n        <mxGeometry x="20" y="20" width="80" height="40" as="geometry" />\n      </mxCell>\n    </Setting>\n    <Stock name="N" Note="Questa è la popolazione batterica la cui dimensione (in unità di cellule vitali) cambierà nel corso del tempo. Per rendere realistica la simulazione considera che la massa (umida) tipica di una cellula di &lt;i&gt;Escherichia coli&lt;/i&gt; è circa un femtogrammo (cioè 10^-12 g, &lt;a rel=&quot;noopener noreferrer nofollow&quot; rel=&quot;noopener noreferrer nofollow&quot; href=&quot;https://www.ncbi.nlm.nih.gov/pmc/articles/PMC106103/&quot; target=&quot;_blank&quot;&gt;anche se può variare&lt;/a&gt;) e che l’&lt;a rel=&quot;noopener noreferrer nofollow&quot; rel=&quot;noopener noreferrer nofollow&quot; href=&quot;https://aem.asm.org/content/aem/48/4/755.full.pdf&quot; target=&quot;_blank&quot;&gt;umidità tipica di una cellula batterica è intorno&lt;/a&gt; al 70%. Quindi, 1 kg di cellule umide contiene 10^12*10^3 = 10^15 cellule. Se tutta l’acqua presente sulla terra (1,4x10^9 km^3, equivalenti a 1,4*10^18 kg) fosse trasformata in batteri sarebbe possibile avere al massimo 2x10^18*10^15 =2x10^33 cellule. Il numero iniziale è 100 batteri, ma puoi cambiarlo con uno slider." InitialValue="[N0]" StockMode="Store" Delay="10" Volume="100" NonNegative="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100000" SliderMin="10" SliderStep="10" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" AllowNegatives="false" id="55">\n      <mxCell style="stock" parent="1" vertex="1">\n        <mxGeometry x="520" y="340" width="100" height="40" as="geometry" />\n      </mxCell>\n    </Stock>\n    <Variable name="Mumax" Note="La costante di velocità di crescita massima: qui si assume che sia costante, perché le condizioni che la possono influenzare sono costanti. In generale, valori ragionevoli sono fra 0.01 e 4 h^-1. Per i microrganismi di interesse alimentare puoi usare ComBase browser o ComBase Predictor (http://www.combase.cc) per cercare valori ragionevoli" Equation="2" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="true" SliderMax="4" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="59">\n      <mxCell style="variable;fontSize=12;" parent="1" vertex="1">\n        <mxGeometry x="80" y="170" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Variable name="V" Note="La velocità assoluta di crescita, qui dipende dalla velocità specifica di crescita [mu] e dal numero di cellule [N] al tempo t.&amp;nbsp;" Equation="[Mu]*[N]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="62">\n      <mxCell style="variable;fontSize=12;" parent="1" vertex="1">\n        <mxGeometry x="410" y="200" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="65">\n      <mxCell style="link" parent="1" source="62" target="70" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="570" y="255" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="66">\n      <mxCell style="link" parent="1" source="55" target="62" edge="1">\n        <mxGeometry x="-40" width="100" height="100" as="geometry">\n          <mxPoint x="530" y="340" as="sourcePoint" />\n          <mxPoint x="530" y="260" as="targetPoint" />\n          <Array as="points" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Text name="D model, curve di Richards (2)" LabelPosition="Middle" Note="Il modello rappresenta la curva di crescita di microrganismi unicellulari che si dividono per scissione binaria:&lt;div&gt;&lt;ol&gt;&lt;li&gt;La popolazione [N] cresce con una cinetica del primo ordine : [V] dipende da una costante ([Mu], &amp;nbsp;la velocità specifica di crescita) e dalla dimensione della popolazione [N]&amp;nbsp;&lt;/li&gt;&lt;li&gt;La crescita è limitata e [N] può raggiungere al massimo un valore Nmax.&amp;nbsp;&lt;/li&gt;&lt;li&gt;Mu dipende inoltre da una funzione di adattamento alpha(t) (qui at), che varia fra ~0 e 1 e che dipende dall&#39;accumulo di un composto essenziale con una cinetica di Michaelis e Menten; la concentrazione iniziale del composto essenziale P0 dipende dall&#39;ambiente di provenienza delle cellule e la quantità q0=P0/Kp rappresenta lo stato fisiologico iniziale delle cellule&lt;/li&gt;&lt;li&gt;lo stato fisiologico iniziale è qui definito dalla quantità h0 = -ln(alpha0) = Mumax*lag&lt;/li&gt;&lt;/ol&gt;&lt;/div&gt;" id="67">\n      <mxCell style="text;fontStyle=0;align=left;fontSize=24;" parent="1" vertex="1">\n        <mxGeometry x="400" y="30" width="345" height="50" as="geometry" />\n      </mxCell>\n    </Text>\n    <Variable name="Log10N" Note="Il logaritmo decimale del numero di batteri" Equation="Log([N])" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="68">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="370" y="400" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="69">\n      <mxCell style="link" parent="1" source="55" target="68" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Flow name="Crescita" Note="" FlowRate="[V]" OnlyPositive="true" TimeIndependent="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" id="70">\n      <mxCell style="flow" parent="1" target="55" edge="1">\n        <mxGeometry x="-1" width="100" height="100" as="geometry">\n          <mxPoint x="569" y="150" as="sourcePoint" />\n          <mxPoint x="-1" y="100" as="targetPoint" />\n          <Array as="points" />\n        </mxGeometry>\n      </mxCell>\n    </Flow>\n    <Variable name="FCB" Note="È il fattore di conversione da numero di cellule a biomassa umida in g, le unità sono cellule/g; per semplicità Il valore è 1X10^12. Un valore frequentemente utilizzato per il peso umido di una singola cellula &lt;i&gt;Escherichia coli&lt;/i&gt; è 9,5*10^-13 g (&lt;a rel=&quot;noopener noreferrer nofollow&quot; rel=&quot;noopener noreferrer nofollow&quot; href=&quot;https://www.ncbi.nlm.nih.gov/pmc/articles/PMC106103/&quot; target=&quot;_blank&quot;&gt;anche se può variare&lt;/a&gt;)." Equation="9.5*10^-13" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="72">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="720" y="510" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Variable name="Biomassa" Note="La biomassa cellulare, in peso umido, in g (per coerenza)" Equation="[N]*[FCB]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="73">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="640" y="430" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="74">\n      <mxCell style="link" parent="1" source="72" target="73" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="75">\n      <mxCell style="link" parent="1" source="55" target="73" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Display name="N vs t" Note="" Type="Time Series" xAxis="Time (%u)" yAxis="N" yAxis2="LogN" showMarkers="false" showLines="true" showArea="false" ThreeDimensional="false" Primitives="55" Primitives2="68,105" AutoAddPrimitives="false" ScatterplotOrder="X Primitive, Y Primitive" Image="Display" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" legendPosition="Automatic" yAxisMin2="0" id="76">\n      <mxCell style="display" parent="1" vertex="1" visible="0">\n        <mxGeometry x="10" y="10" width="64" height="64" as="geometry" />\n      </mxCell>\n    </Display>\n    <Picture name="Bacteria" Note="" Image="https://www.microscopemaster.com/images/e.coliunderelectornmicroscope.jpg" FlipHorizontal="false" FlipVertical="false" LabelPosition="Bottom" id="78">\n      <mxCell style="picture;image=https://www.microscopemaster.com/images/e.coliunderelectornmicroscope.jpg;imageFlipV=0;imageFlipH=0;shape=image;" parent="1" vertex="1">\n        <mxGeometry x="660" y="230" width="180" height="160" as="geometry" />\n      </mxCell>\n    </Picture>\n    <Variable name="Nmax" Note="È il numero massimo che i batteri possono raggiungere. Qui, pur essendo un valore arbitrario, è un valore vicino a quello che potrebbe essere la popolazione massima per g o ml di alimento deteriorato.&amp;nbsp;In questa fase non facciamo ipotesi su che cosa determini questa limitazione e quali possano essere per esempio le relazioni con la disponibilità di substrato." Equation="10^9" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="true" SliderMax="10000000000" SliderMin="1000000" SliderStep="100000" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="79">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="80" y="80" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Variable name="Mu" Note="La velocità specifica di crescita all&#39;istante t con unità tempo^-1. Varia in funzione di Mumax (che è costante se le condizioni ambientali sono costanti), N e Nmax. (1-N/Nmax) ha un valore molto vicino a 1 quando N&amp;lt;&amp;lt;Nmax mentre si avvicina a 0 se N-&amp;gt;Nmax. Naturalmente, la velocità specifica reale non è mai massima e questo contraddice molte osservazioni sperimentali. Inoltre, anche l&#39;uso di una relazione logistica determina un comportamento che si allontana dalle condizioni reali (molto spesso la transizione fra un valore prossimo a Mumax e 0 può essere molto più veloce o lento di quanto si può prevedere sulla base della relazione logistica." Equation="[Mumax]*[at]*(1-([N]/[Nmax])^[m])" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="81">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="260" y="150" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="82">\n      <mxCell style="link" parent="1" source="81" target="62" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="83">\n      <mxCell style="link" parent="1" source="79" target="81" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="84">\n      <mxCell style="link" parent="1" source="59" target="81" edge="1">\n        <mxGeometry x="3.74" y="19.76" width="100" height="100" as="geometry">\n          <mxPoint x="3.74" y="119.75999999999999" as="sourcePoint" />\n          <mxPoint x="103.74000000000001" y="19.76" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="85">\n      <mxCell style="link" parent="1" source="55" target="81" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n          <Array as="points">\n            <mxPoint x="350" y="330" />\n          </Array>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="m" Note="È un coefficiente positivo (valori ragionevoli sono fra 0.2 e 10) che influenza la transizione di Mu all&#39;approssimarsi di N a Nmax. Se vale 1 la curva è identica ad una logistica." Equation="1" Units="Unitless" MaxConstraintUsed="true" MinConstraintUsed="true" MaxConstraint="10" MinConstraint="0.2" ShowSlider="true" SliderMax="10" SliderMin="0.2" SliderStep="0.05" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="86">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="220" y="280" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="87">\n      <mxCell style="link" parent="1" source="86" target="81" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="lNN0" Note="Il logaritmo (naturale) del rapporto fra il numero iniziale di cellule e il numero al tempo t. E&#39; un modo per esprimere l&#39;incremento del numero di cellule in maniera adimensionale." Equation="Ln([N]/[N0])" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="88">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="570" y="510" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="89">\n      <mxCell style="link" parent="1" source="55" target="88" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="N0" Note="il numero iniziale di cellule, uso una variabile per convenienza" Equation="1000" Units="Unitless" MaxConstraintUsed="true" MinConstraintUsed="true" MaxConstraint="1000000000" MinConstraint="1" ShowSlider="true" SliderMax="1000000" SliderMin="100" SliderStep="100" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="90">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="410" y="510" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="91">\n      <mxCell style="link" parent="1" source="90" target="55" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="92">\n      <mxCell style="link" parent="1" source="90" target="88" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="at" Note="la funzione di adattamento che determina la fase lag, nella transizione fra l&#39;ambiente di provenienza E1 delle cellule e il nuovo ambiente E2. Varia fra 0 e 1." Equation="[q]/(1+[q])" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="93">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="80" y="280" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="94">\n      <mxCell style="link" parent="1" source="93" target="81" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Stock name="q" Note="La quantità q(t) che rappresenta la variazione nel tempo di P(t)/Kp dove P(t) è la concentrazione del generico composto essenziale P (potrebbe essere un enzima indicibile, il numero di ribosomi, etc.) e Kp è la costante di Michaelis e Menten per l&#39;equazione che regola l&#39;accumulo di P" InitialValue="[q0]" StockMode="Store" Delay="10" Volume="100" NonNegative="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="95">\n      <mxCell style="stock" parent="1" vertex="1">\n        <mxGeometry x="80" y="670" width="100" height="40" as="geometry" />\n      </mxCell>\n    </Stock>\n    <Variable name="q0" Note="Una quantità che rappresenta lo stato fisiologico iniziale delle cellule, P0/Kp. Qui si usa il valore usato da Baranyi e Roberts nel lavoro del 1984. Valori ragionevoli potrebbero essere fra 0.001 e 1000 (lag duration praticamente 0)." Equation="1/(Exp([h0])-1)" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="true" SliderMax="1000" SliderMin="0.001" SliderStep="0.001" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="96">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="210" y="590" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="97">\n      <mxCell style="link" parent="1" source="96" target="95" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="98">\n      <mxCell style="link" parent="1" source="95" target="93" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Flow name="Vq" Note="la velocità assoluta di accumulo di q" FlowRate="[ni]*[q]" OnlyPositive="true" TimeIndependent="false" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" id="99">\n      <mxCell style="flow" parent="1" target="95" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint x="450" y="690" as="sourcePoint" />\n          <mxPoint y="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Flow>\n    <Ghost Source="59" name="Mumax" id="100">\n      <mxCell style="variable;fontSize=12;opacity=30;" parent="1" vertex="1">\n        <mxGeometry x="410" y="760" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Ghost>\n    <Variable name="ni" Note="la velocità specifica di accumulo di q nell&#39;ambiente E2, qui si pone uguale a mumax" Equation="[Mumax]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="101">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="210" y="760" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="102">\n      <mxCell style="link" parent="1" source="100" target="101" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="103">\n      <mxCell style="link" parent="1" source="101" target="99" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Link name="Link" Note="" BiDirectional="false" id="104">\n      <mxCell style="link" parent="1" source="95" target="99" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n          <Array as="points">\n            <mxPoint x="230" y="760" />\n          </Array>\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="lag duration" Note="La durata della fase lag, dipende da q0 e in (e quindi mumax), in unità di tempo" Equation="[h0]/[ni]" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="false" SliderMax="100" SliderMin="0" SliderStep="" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="105">\n      <mxCell style="variable" parent="1" vertex="1">\n        <mxGeometry x="540" y="660" width="120" height="50" as="geometry" />\n      </mxCell>\n    </Variable>\n    <Link name="Link" Note="" BiDirectional="false" id="107">\n      <mxCell style="link" parent="1" source="101" target="105" edge="1">\n        <mxGeometry width="100" height="100" as="geometry">\n          <mxPoint y="100" as="sourcePoint" />\n          <mxPoint x="100" as="targetPoint" />\n        </mxGeometry>\n      </mxCell>\n    </Link>\n    <Variable name="h0" Note="un&#39;altra quantità che rappresenta lo stato iniziale della cellula. Graficamente, è la differenza fra logn(N0) e l&#39;intercetta sull&#39;asse delle y (N o logN) della tangente al punto di massima pendenza della curva di crescita. Per calcolo si vede facilmente che h0 = [Mumax]*[lag duration].&lt;div&gt;Generalmente per cellule cresciute nelle stesse condizioni, è una costante, quindi la durata della fase lag è inversamente proporzionale alla velocità specifica di crescita. h0 può essere calcolato da q0 o viceversa. Qui si calcola q0 da h0 per poter determinare lag duration in funzione di&amp;nbsp;[Mumax]&amp;nbsp;(e quindi calcolare più facilmente la durata della fase lag quando&amp;nbsp;[Mumax]&amp;nbsp;è variabile perché influenzata dalla temperatura o dal pH). h0 può avere valori compresi fra 1.31 (calcolato da Baranyi e Pin, 1999 per Pseudomonas) a 3,2 (Baranyi e Roberts, 1994) a 2.7 (Augustin e Carlier, 2000, per L. monocytogenes).&lt;/div&gt;&lt;div&gt;&lt;br&gt;&lt;/div&gt;&lt;div&gt;&lt;br class=&quot;ng-star-inserted&quot;&gt;&lt;i class=&quot;ng-star-inserted&quot;&gt;Augustin, J., Carlier, V.&lt;/i&gt;&lt;span style=&quot;font-size: medium&quot;&gt;&amp;nbsp;(2000).&amp;nbsp;&lt;/span&gt;&lt;b class=&quot;ng-star-inserted&quot;&gt;Mathematical modelling of the growth rate and lag time for Listeria monocytogenes&lt;/b&gt;&lt;span style=&quot;font-size: medium&quot;&gt;International journal of food microbiology&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;56(1)&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;, 29 - 51&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;.&amp;nbsp;&lt;/span&gt;&lt;a target=&quot;_blank&quot; href=&quot;https://dx.doi.org/10.1016/s0168-1605(00)00223-3&quot; class=&quot;ng-star-inserted&quot; rel=&quot;noopener noreferrer nofollow&quot;&gt;https://dx.doi.org/10.1016/s0168-1605(00)00223-3&lt;/a&gt;&lt;/div&gt;&lt;div&gt;&lt;i class=&quot;ng-star-inserted&quot;&gt;&lt;i class=&quot;ng-star-inserted&quot;&gt;Baranyi, J., Pin, C.&lt;/i&gt;&lt;span style=&quot;font-size: medium&quot;&gt;&amp;nbsp;(1999).&amp;nbsp;&lt;/span&gt;&lt;b class=&quot;ng-star-inserted&quot;&gt;Estimating Bacterial Growth Parameters by Means of Detection Times&lt;/b&gt;&lt;span style=&quot;font-size: medium&quot;&gt;&amp;nbsp;Applied and Environmental Microbiology&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;65(2)&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;, 732-736&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;.&amp;nbsp;&lt;/span&gt;&lt;a target=&quot;_blank&quot; href=&quot;https://dx.doi.org/10.1128/aem.65.2.732-736.1999&quot; class=&quot;ng-star-inserted&quot; rel=&quot;noopener noreferrer nofollow&quot;&gt;https://dx.doi.org/10.1128/aem.65.2.732-736.1999&lt;/a&gt;&lt;/i&gt;&lt;/div&gt;&lt;div&gt;&lt;i class=&quot;ng-star-inserted&quot;&gt;Baranyi, J., Roberts, T.&lt;/i&gt;&lt;span style=&quot;font-size: medium&quot;&gt;&amp;nbsp;(1994).&amp;nbsp;&lt;/span&gt;&lt;b class=&quot;ng-star-inserted&quot;&gt;A dynamic approach to predicting bacterial growth in food&lt;/b&gt;&lt;span style=&quot;font-size: medium&quot;&gt;&amp;nbsp;International journal of food microbiology&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;23()&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;, 1 - 18&lt;/span&gt;&lt;span style=&quot;font-size: medium&quot;&gt;.&amp;nbsp;&lt;/span&gt;&lt;br&gt;&lt;/div&gt;" Equation="2" Units="Unitless" MaxConstraintUsed="false" MinConstraintUsed="false" MaxConstraint="100" MinConstraint="0" ShowSlider="true" SliderMax="4" SliderMin="0.5" SliderStep="0.05" Image="None" FlipHorizontal="false" FlipVertical="false" LabelPosition="Middle" id="108">\n      <mxCell style